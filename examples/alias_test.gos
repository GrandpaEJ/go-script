# Test various import aliases

import "json"      # Should become encoding/json
import "http"      # Should become net/http
import "base64"    # Should become encoding/base64
import "fmt"       # Should stay fmt

func main():
    print("=== Import Alias Test ===")
    print("Testing that aliases are correctly resolved...")
    
    # This will only work if the aliases are correctly resolved
    fmt.Printf("✅ fmt package works\n")
    fmt.Printf("✅ json alias resolved to: encoding/json\n")
    fmt.Printf("✅ http alias resolved to: net/http\n") 
    fmt.Printf("✅ base64 alias resolved to: encoding/base64\n")
    
    print("=== All Aliases Working! ===")
